datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Property {
  id          String   @id @default(uuid())
  city        String
  street      String
  state       String
  zipCode     String
  weatherData Json
  lat         Float?
  long        Float?
  createdAt   DateTime @default(now())
}
